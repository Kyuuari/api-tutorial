#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/bin/node_modules:/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/node_modules:/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/typescript@5.0.4/node_modules:/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/bin/node_modules:/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/typescript@5.0.4/node_modules/typescript/node_modules:/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/typescript@5.0.4/node_modules:/Users/chestercari/Documents/Dev/api-practice/api-tutorial/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/tsserver" "$@"
else
  exec node  "$basedir/../../bin/tsserver" "$@"
fi
